                <div class="container-fluid">
                    <div class="row mb-2">
                        <div class="col-sm-6">
                            <h1 class="m-0">Personal</h1>
                        </div><!-- /.col -->

                    </div><!-- /.row -->
                </div><!-- /.container-fluid -->
            </div>
            <!-- /.content-header -->
            <!-- Main content -->
            <section class="content">
                <div class="container-fluid">
                    <div class="row">
                        <!-- left column -->
                        <div class="col-md-12">



                            <!-- Horizontal Form -->
                            <div class="card  card-dark">
                                <div class="card-header">
                                    <h3 class="card-title">Registrar Personal</h3>
                                </div>
                                <!-- /.card-header -->
                                <!-- form start -->
                                <form class="form-horizontal" method="post">


                                    <div class="card-body">

                                        <div class="form-group row">
                                            <label class="col-sm-2 col-form-label">Nombre :</label>
                                            <div class="col-sm-10">
                                                <input asp-for="RegistrarPersonal.Nombre" class="form-control" placeholder="Nombre">
                                                <span asp-validation-for="RegistrarPersonal.Nombre" style="color:darkred"></span>
                                            </div>
                                        </div>






                                        <div class="form-group row">
                                            <label class="col-sm-2 col-form-label">Apellidos :</label>
                                            <div class="col-sm-10">
                                                <input asp-for="RegistrarPersonal.Apellidos" class="form-control" placeholder="Apellidos">
                                                <span asp-validation-for="RegistrarPersonal.Apellidos" style="color:darkred"></span>
                                            </div>
                                        </div>





                                        <div class="form-group row">
                                            <label class="col-sm-2 col-form-label">Fecha de Nacimiento :</label>
                                            <div class="col-sm-10">
                                                <input type="date" asp-for="RegistrarPersonal.Fecha" class="form-control">
                                                <span asp-validation-for="RegistrarPersonal.Fecha" style="color:darkred"></span>

                                            </div>
                                        </div>





                                        <div class="form-group row">
                                            <label class="col-sm-2 col-form-label">DNI :</label>
                                            <div class="col-sm-10">
                                                <input asp-for="RegistrarPersonal.Dni" class="form-control" placeholder="DNI">
                                                <span asp-validation-for="RegistrarPersonal.Dni" style="color:darkred"></span>
                                            </div>
                                        </div>



                                       



                                        <div class="form-group row">
                                            <label class="col-sm-2 col-form-label">Correo :</label>
                                            <div class="col-sm-10">
                                                <input type="email" asp-for="RegistrarPersonal.Correo" class="form-control" placeholder="Correo">
                                                <span asp-validation-for="RegistrarPersonal.Correo" style="color:darkred"></span>
                                            </div>
                                        </div>



                                        <div class="form-group row">
                                            <label class="col-sm-2 col-form-label">Celular :</label>
                                            <div class="col-sm-10">
                                                <input asp-for="RegistrarPersonal.Cel" class="form-control" placeholder="Celular">
                                                <span asp-validation-for="RegistrarPersonal.Cel" style="color:darkred"></span>
                                            </div>
                                        </div>

                                        

                                    </div>


                                    <!-- /.card-body -->
                                    <div class="card-footer">
                                        <button type="submit" class="btn btn-success">Registrar</button>

                                    </div>
                                    <!-- /.card-footer -->
                                </form>
                            </div>
                            <!-- /.card -->

                        </div>

